{
  "name": "finflow-backend",
  "version": "1.0.0",
  "description": "FinFlow Phase-1 Backend - Customer Booking & Loan Tracking App",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
    "lint": "eslint .",
    "backfill:dropbox": "node src/scripts/backfillDropboxIds.js",
    "repair:dropbox": "node src/scripts/repairOrphanedDropboxCustomers.js",
    "check:cors": "PORT=${PORT:-5000} ORIGIN=https://shubhadevelopers.com sh -c 'curl -i -X OPTIONS \"http://localhost:$PORT/api/customers\" -H \"Origin: $ORIGIN\" -H \"Access-Control-Request-Method: GET\" -H \"Access-Control-Request-Headers: authorization,content-type\"'"
  },
  "keywords": [
    "express",
    "postgres",
    "loan-tracking",
    "customer-management",
    "fintech"
  ],
  "author": "Shubha Developers",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
	"cookie-session": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.5",
    "sequelize": "^6.37.3",
    "winston": "^3.14.0",
    "express-validator": "^6.15.0",
    "dropbox": "^10.34.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
